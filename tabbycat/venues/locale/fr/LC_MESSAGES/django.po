#: venues/utils.py:39 venues/utils.py:51
#, python-format
msgid "Venue does not meet any constraint of %(name)s"
msgstr "Ce lieu ne satisfait aucune contrainte de %(name)s"

#: venues/utils.py:44
#, python-format
msgid ""
"Venue constraint of %(team)s met (%(category)s, via institution "
"%(institution)s)"
msgstr ""
"Contrainte de lieu de %(team)s est satisfaite (%(category)s, via institution"
" %(institution)s)"

#: venues/utils.py:45
#, python-format
msgid ""
"Venue does not meet any constraint of institution %(institution)s (%(team)s)"
msgstr ""
"Ce lieu ne satisfait aucune contrainte de l'institution %(institution)s "
"(%(team)s)"

