if [ -z "${SITE_ROOT+x}" ]; then
    SITE_ROOT="$( cd "$(dirname ${BASH_SOURCE[0]})/.." && pwd )"
    export SITE_ROOT
    echo "The site root is $SITE_ROOT";
fi

source $SITE_ROOT/env/bin/activate
export PYTHONPATH=$PYTHONPATH:$SITE_ROOT/src/
export PATH=$SITE_ROOT/bin:$PATH

export DJANGO_SETTINGS_MODULE="australs.settings"

